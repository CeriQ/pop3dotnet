{
  "title": "Pop3.NET",
  "description": "Small and simple library for retrieving messages from Post Office Protocol version 3 (POP3) servers.",
  "version": "2.1.1-0",
  "copyright": "Rodolfo Finochietti",
  "dependencies": {
    "NETStandard.Library": "1.6.0",
    "System.Net.Security": "4.0.0-*"
  },
  "frameworks": {
    "netstandard1.6": {
      "imports": "dnxcore50"
    }
  },
  "compilationOptions": {
    "keyFile": "..\\Resources\\Key\\Pop3.snk",
    "useOssSigning": true
  },
  "buildOptions": {
    "outputName": "Pop3",
    "defines": [ "CORE" ]
  }
}